<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nchu.mapper.UserVideoMapper">

    <resultMap id="BaseResultMap" type="com.nchu.pojo.UserVideo">
            <id property="userid" column="userid" jdbcType="INTEGER"/>
            <id property="videoid" column="videoid" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        userid,videoid
    </sql>
    <select id="queryByPage" resultType="java.lang.Integer">
        select videoid from user_video
        where userid = #{userid}
    </select>
</mapper>
